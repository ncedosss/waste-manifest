<service>
	<id>wacomagent.exe</id>
	<name>WacomAgent</name>
	<description>A signature pad agent</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\package\WacomAgent\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\package\WacomAgent\local-agent.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>WacomAgent wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>DESKTOP-4V45J0U</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\package\WacomAgent</workingdirectory>
</service>